<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">
    <TextView
        style="@style/textBoldDrawableStyle"
        android:layout_marginTop="12dp"
        android:text="名称" />

    <EditText
        android:id="@+id/et_name"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入名称"
        />

    <TextView
        style="@style/textBoldDrawableStyle"
        android:layout_marginTop="12dp"
        android:text="产权单位名称" />

    <EditText
        android:id="@+id/et_danweiName"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入产权单位名称"
        />

    <TextView
        style="@style/textBoldStyle"
        android:layout_marginTop="12dp"
        android:text="产权单位法定代表人" />

    <EditText
        android:id="@+id/et_farenName"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入产权单位法定代表人"
        />
    <TextView
        style="@style/textBoldStyle"
        android:layout_marginTop="12dp"
        android:text="产权单位联系人" />

    <EditText
        android:id="@+id/et_chanquanDanweiName"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入产权单位联系人"
        />
    <TextView
        style="@style/textBoldStyle"
        android:layout_marginTop="12dp"
        android:text="产权单位联系人电话" />

    <EditText
        android:id="@+id/et_phone"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入产权单位联系人电话"
        />

    <TextView
        style="@style/textBoldDrawableStyle"
        android:layout_marginTop="12dp"
        android:text="管理使用单位名称" />

    <EditText
        android:id="@+id/et_guanlishiyongDanweiName"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入管理使用单位名称"
        />
    <TextView
        style="@style/textBoldStyle"
        android:layout_marginTop="12dp"
        android:text="管理使用单位法定代表人" />

    <EditText
        android:id="@+id/et_guanlishiyongFarenName"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入管理使用单位法定代表人"
        />
    <TextView
        style="@style/textBoldStyle"
        android:layout_marginTop="12dp"
        android:text="管理使用单位联系人" />

    <EditText
        android:id="@+id/et_guanlishiyongLianxiName"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入管理使用单位联系人"
        />
    <TextView
        style="@style/textBoldStyle"
        android:layout_marginTop="12dp"
        android:text="管理使用单位联系人电话" />

    <EditText
        android:id="@+id/et_guanlishiyongContact"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入管理使用单位联系人电话"
        />


</LinearLayout>