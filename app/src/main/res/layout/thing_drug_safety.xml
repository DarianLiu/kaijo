<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
        style="@style/textBoldDrawableStyle"
        android:layout_marginTop="12dp"
        android:text="企业名称" />

    <EditText
        android:id="@+id/et_companyName"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入企业名称"
        />

    <TextView
        style="@style/textBoldDrawableStyle"
        android:layout_marginTop="12dp"
        android:text="注册地址" />

    <EditText
        android:id="@+id/et_register_name"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入注册地址"
        />

    <TextView
        style="@style/textBoldDrawableStyle"
        android:layout_marginTop="12dp"
        android:text="企业法定代表人" />

    <EditText
        android:id="@+id/et_farenName"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入企业法定代表人"
        />

    <TextView
        style="@style/textBoldStyle"
        android:layout_marginTop="12dp"
        android:text="住店药师" />

    <EditText
        android:id="@+id/et_yaoshi"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入住店药师"
        />

    <TextView
        style="@style/textBoldDrawableStyle"
        android:layout_marginTop="12dp"
        android:text="许可证号" />

    <EditText
        android:id="@+id/et_xukezhanghao"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入许可证号"
        />

    <TextView
        style="@style/textBoldDrawableStyle"
        android:layout_marginTop="12dp"
        android:text="许可证颁发时间" />

    <TextView
        android:id="@+id/et_xukezhengTime"
        style="@style/textTimeStyle"
        android:layout_marginTop="5dp"
        />

    <TextView
        style="@style/textBoldDrawableStyle"
        android:layout_marginTop="12dp"
        android:text="有效期至" />

    <TextView
        android:id="@+id/et_youxiaoqiTime"
        style="@style/textTimeStyle"
        android:layout_marginTop="5dp"
        />

    <TextView
        style="@style/textBoldStyle"
        android:layout_marginTop="12dp"
        android:text="经营方式" />

    <EditText
        android:id="@+id/et_jingyinfanshi"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入经营方式"
        />

    <TextView
        style="@style/textBoldStyle"
        android:layout_marginTop="12dp"
        android:text="经营范围" />

    <EditText
        android:id="@+id/et_jingyinfanwei"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入经营范围"
        />

    <TextView
        style="@style/textBoldStyle"
        android:layout_marginTop="12dp"
        android:text="联系电话" />

    <EditText
        android:id="@+id/et_Telephone"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入联系电话"
        />

    <TextView
        style="@style/textBoldStyle"
        android:layout_marginTop="12dp"
        android:text="手机号" />

    <EditText
        android:id="@+id/et_phone"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入手机号"
        />



</LinearLayout>