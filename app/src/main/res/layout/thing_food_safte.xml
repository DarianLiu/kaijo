<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
        style="@style/textBoldDrawableStyle"
        android:layout_marginTop="12dp"
        android:text="经营者名称" />

    <EditText
        android:id="@+id/et_name"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入请经营者名称"
        />

    <TextView
        style="@style/textBoldDrawableStyle"
        android:layout_marginTop="12dp"
        android:text="法定代表负责人" />

    <EditText
        android:id="@+id/et_farenName"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入法定代表负责人"
        />

    <TextView
        style="@style/textBoldStyle"
        android:layout_marginTop="12dp"
        android:text="经济性质" />

    <EditText
        android:id="@+id/et_jjxz"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入经济性质"
        />

    <TextView
        style="@style/textBoldDrawableStyle"
        android:layout_marginTop="12dp"
        android:text="经营场所" />

    <EditText
        android:id="@+id/et_jycs"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入经营场所"
        />

    <TextView
        style="@style/textBoldDrawableStyle"
        android:layout_marginTop="12dp"
        android:text="许可证号" />

    <EditText
        android:id="@+id/et_xkzh"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入许可证号"
        />

    <TextView
        style="@style/textBoldStyle"
        android:layout_marginTop="12dp"
        android:text="主体业态" />

    <EditText
        android:id="@+id/et_ztyt"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入主体业态"
        />
    <TextView
        style="@style/textBoldStyle"
        android:layout_marginTop="12dp"
        android:text="是否网络经营" />

    <android.support.v7.widget.AppCompatSpinner
        android:id="@+id/spinner_street"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp" />

    <TextView
        style="@style/textBoldDrawableStyle"
        android:layout_marginTop="12dp"
        android:text="有效期至" />

    <TextView
        android:id="@+id/tv_time"
        style="@style/textTimeStyle"
        android:layout_marginTop="5dp" />

    <TextView
        style="@style/textBoldStyle"
        android:layout_marginTop="12dp"
        android:text="主体类别" />

    <EditText
        android:id="@+id/et_category"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入主体类别"
        />


    <TextView
        style="@style/textBoldStyle"
        android:layout_marginTop="12dp"
        android:text="监管机构" />

    <EditText
        android:id="@+id/et_jgjg"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入监管机构"
        />

    <TextView
        style="@style/textBoldStyle"
        android:layout_marginTop="12dp"
        android:text="风险等级" />

    <EditText
        android:id="@+id/et_fxdj"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入风险等级"
        />

    <TextView
        style="@style/textBoldStyle"
        android:layout_marginTop="12dp"
        android:text="联系电话" />

    <EditText
        android:id="@+id/et_Telephone"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入联系电话"
        />

    <TextView
        style="@style/textBoldStyle"
        android:layout_marginTop="12dp"
        android:text="手机号码" />

    <EditText
        android:id="@+id/et_phone"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入手机号码"
        />


</LinearLayout>