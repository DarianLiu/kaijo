<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
        style="@style/textBoldDrawableStyle"
        android:layout_marginTop="12dp"
        android:text="类别" />

    <TextView
        android:id="@+id/spinner_category"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        />

    <TextView
        style="@style/textBoldStyle"
        android:layout_marginTop="12dp"
        android:text="状态" />

    <TextView
        android:id="@+id/et_state"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入状态"
        />

    <TextView
        style="@style/textBoldDrawableStyle"
        android:layout_marginTop="12dp"
        android:text="位置" />

    <TextView
        android:id="@+id/et_address"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入位置"
        />

    <TextView
        style="@style/textBoldStyle"
        android:layout_marginTop="12dp"
        android:text="产权单位（管理单位）" />

    <TextView
        android:id="@+id/et_danweiName"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入产权单位（管理单位）"
        />

    <TextView
        style="@style/textBoldStyle"
        android:layout_marginTop="12dp"
        android:text="产权单位（管理单位）责任人" />

    <TextView
        android:id="@+id/et_farenName"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入产权单位（管理单位）责任人"
        />

    <TextView
        style="@style/textBoldStyle"
        android:layout_marginTop="12dp"
        android:text="产权单位责任人联系电话" />

    <TextView
        android:id="@+id/et_leading_TlPhone"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入产权单位责任人联系电话"
        />


    <TextView
        style="@style/textBoldStyle"
        android:layout_marginTop="12dp"
        android:text="产权单位责任人联系手机" />

    <TextView
        android:id="@+id/et_leading_phone"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入产权单位责任人联系手机"
        />
    <TextView
        style="@style/textBoldStyle"
        android:layout_marginTop="12dp"
        android:text="街道责任人" />

    <TextView
        android:id="@+id/et_jiedaozerenName"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入街道责任人"
        />
    <TextView
        style="@style/textBoldStyle"
        android:layout_marginTop="12dp"
        android:text="街道责任人联系电话" />

    <TextView
        android:id="@+id/jiedaozerenMobile"
        style="@style/editDefalutStyle"
        android:layout_marginTop="5dp"
        android:hint="请输入街道责任人联系电话"
        />


